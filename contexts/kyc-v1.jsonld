{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    
    "id": "@id",
    "type": "@type",
    
    "FinancialCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#FinancialCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName",

	"sex": "kyc:sex",
        "birthDate": { "@id": "kyc:birthDate", "@type": "xsd:dateTime" },
	"birthPlace": "kyc:birthPlace",
        "nationality": "kyc:nationality",

        "driverLicenseId": "kyc:driverLicenseId",
	"idCardNumber": "kyc:idCardNumber",
        "passportNumber": "kyc:passportNumber",
        "socialSecurityNumber": "kyc:socialSecurityNumber",

        "emailAddress": "kyc:emailAddress",
        "phoneNumber": "kyc:phoneNumber",

        "residentialAddress": { 
          "@id": "kyc:ResidentialAddressCredential",
          "@type": "kyc:ResidentialAddressCredential"
        },
        "registrationAddressDetails": { 
          "@id": "kyc:RegistrationAddressDetails",
          "@type": "kyc:RegistrationAddressDetails"
        }
      }
    },

    "DriverLicenseCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#DriverLicenceCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "driverLicenseId": "kyc:driverLicenseId",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName",

        "sex": "kyc:sex",
        "birthDate": "kyc:birthDate",

        "issueDate": "kyc:issueDate",
        "expiryDate": "kyc:expiryDate"    
      }
    },
	  
    "IdCardCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#IdCardCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "idCardNumber": "kyc:idCardNumber",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName",

        "sex": "kyc:sex",
        "birthDate": "kyc:birthDate",

        "issueDate": "kyc:issueDate",
        "expiryDate": "kyc:expiryDate"
      }
    },

    "PassportCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#PassportCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "passportCountry": "kyc:passportCountry",
        "passportNumber": "kyc:passportNumber",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName",

	"sex": "kyc:sex",
        "birthDate": "kyc:birthDate",
        "birthPlace": "kyc:birthPlace",
        "nationality": "kyc:nationality",

        "issueAuthority": "kyc:issueAuthority",
        "issueDate": "kyc:issueDate",
        "expiryDate": "kyc:expiryDate"
      }
    },

    "SocialSecurityCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#SocialSecurityCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "socialSecurityNumber": "kyc:socialSecurityNumber",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName"
      }
    },
	  
    "SelfSignedIdCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#SelfSignedIdCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,
	  
        "id": "@id",
        "type": "@type"
      }
    },
	  
    "EmailAddressCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#EmailAddressCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "emailAddress": "kyc:emailAddress"
      }
    },

    "PhoneNumberCredential": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#PhoneNumberCredential",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "phoneNumber": "kyc:phoneNumber"
      }
    },

    ResidentialAddressCredential: {
      '@id': 'https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#ResidentialAddressCredential',
      '@context': {
        '@version': 1.1,
        '@protected': true,

        id: '@id',
        type: '@type',

        kyc: 'https://getportabl.github.io/kyc-vocab#',

        streetAddress: 'kyc:streetAddress',
        locality: 'kyc:locality',
        region: 'kyc:region',
        postalCode: 'kyc:postalCode',
        country: 'kyc:country'
      }
    },

    RegistrationAddressDetails: {
      '@id': 'https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#RegistrationAddressDetails',
      '@context': {
        '@version': 1.1,
        '@protected': true,

        id: '@id',
        type: '@type',

        kyc: 'https://getportabl.github.io/kyc-vocab#',

        streetAddress: 'kyc:streetAddress',
        locality: 'kyc:locality',
        region: 'kyc:region',
        postalCode: 'kyc:postalCode',
        country: 'kyc:country'
      }
    }
  }
}
