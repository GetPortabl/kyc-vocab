{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    
    "id": "@id",
    "type": "@type",
    
    "vKYC": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#vKYC",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "kyc": "https://getportabl.github.io/kyc-vocab#",
        
        "emailAddress": "kyc:emailAddress",
        "phoneNumber": "kyc:phoneNumber",

        "fullName": "kyc:fullName",
        "firstName": "kyc:firstName",
        "middleName": "kyc:middleName",
        "lastName": "kyc:lastName",
        "honorificPrefix": "kyc:honorificPrefix",
        "honorificSuffix": "kyc:honorificSuffix",

        "birthDate": { "@id": "kyc:birthDate", "@type": "xsd:dateTime" },
		    "birthPlace": "kyc:birthPlace",

        "nationality": "kyc:nationality",

        "socialSecurityNumber": "kyc:socialSecurityNumber",

        "registrationAddressFull": "kyc:registrationAddressFull",
        "registrationAddressDetails": { 
          "@id": "kyc:RegistrationAddressDetails",
          "@type": "kyc:RegistrationAddressDetails"
        }

        "driverLicenseId": "kyc:driverLicenseId"
        "passportNumber": "kyc:passportNumber"
      }
    },

    "RegistrationAddressDetails": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#RegistrationAddressDetails",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",
        
        "country": "kyc:country",
        "postalCode": "kyc:postalCode",
        "region": "kyc:region",
        "locality": "kyc:locality",
        "streetAddress": "kyc:streetAddress"
      }
    },

    "DriverLicense": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#DriverLicence",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "driverLicenseId": "kyc:driverLicenseId",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName",

        "birthDate": "kyc:birthDate",

        "issueDate": "kyc:issueDate",
        "expiryDate": "kyc:expiryDate",
        "sex": "kyc:sex"
      }
    },

    "Passport": {
      "@id": "https://getportabl.github.io/kyc-vocab/contexts/kyc-v1.jsonld#Passport",
      "@context": {
        "@version": 1.1,
        "@protected": true,

        "id": "@id",
        "type": "@type",

        "kyc": "https://getportabl.github.io/kyc-vocab#",

        "passportCountry": "kyc:passportCountry",
        "passportNumber": "kyc:passportNumber",

        "firstName": "kyc:firstName",
        "lastName": "kyc:lastName",

        "birthDate": "kyc:birthDate",
        "birthPlace": "kyc:birthPlace",

        "nationality": "kyc:nationality",

        "issueAuthority": "kyc:issueAuthority",
        "issueDate": "kyc:issueDate",
        "expiryDate": "kyc:expiryDate",
      }
    }
  }
}
